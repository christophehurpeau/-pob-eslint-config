{
  "name": "@pob/eslint-plugin",
  "version": "57.0.0",
  "description": "eslint plugin for react",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
