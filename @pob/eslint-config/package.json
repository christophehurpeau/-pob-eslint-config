{
  "name": "@pob/eslint-config",
  "version": "45.0.3",
  "description": "eslint config files",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/eslint-config-pob.git",
    "directory": "@pob/eslint-config"
  },
  "homepage": "https://github.com/christophehurpeau/eslint-config-pob",
  "type": "commonjs",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/base.js",
    "./node-commonjs": "./lib/node-commonjs.js",
    "./node-module": "./lib/node-module.js",
    "./root-commonjs": "./lib/root-commonjs.js",
    "./root-module": "./lib/root-module.js",
    "./prettier": "./lib/prettier.js",
    "./rules/expert": "./lib/rules/expert.js",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn --cwd ../.. run eslint--report-unused-disable-directives --resolve-plugins-relative-to . --quiet @pob/eslint-config"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  },
  "pob": {},
  "peerDependencies": {
    "eslint": "^7.30.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^35.0.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-node": {
      "optional": true
    },
    "eslint-plugin-unicorn": {
      "optional": true
    }
  },
  "dependencies": {
    "@pob/use-eslint-plugin": "^45.0.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^35.0.0"
  },
  "devDependencies": {
    "eslint": "7.32.0"
  }
}
